{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "handle": "filesystem",
          "src": "/public/css/(.*)", 
          "destination": "/public/css/$1"
        }
      ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}

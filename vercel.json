{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/public/css/(.*)",
            "destination": "/public/css/$1"
        },
        {
            "source": "/public/images/(.*)",
            "destination": "/public/images/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}
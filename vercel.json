{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/public/css/(.*)",
          "dest": "/public/css/$1"  
        },
        {
          "src": "/public/images/(.*)",
          "dest": "/public/images/$1"  
        }
      ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}

{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "src": "/public/css/forgot-password.css", 
            "dest": "/public/css/forgot-password.css" 
          },
          {
            "src": "/public/images/login.png", 
            "dest": "/public/images/login.png" 
          },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}